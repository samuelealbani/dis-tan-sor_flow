{
  "extends": ["tslint-no-circular-imports"],
  "rulesDirectory": ".tslint",
  "rules": {
    "array-type": [false, "array-simple"],
    "arrow-return-shorthand": false,
    "ban": [false,
      ["fit"],
      ["fdescribe"],
      ["xit"],
      ["xdescribe"],
      ["fitAsync"],
      ["xitAsync"],
      ["fitFakeAsync"],
      ["xitFakeAsync"]
    ],
    "ban-types": [false,
      ["Object", "Use {} instead."],
      ["String", "Use 'string' instead."],
      ["Number", "Use 'number' instead."],
      ["Boolean", "Use 'boolean' instead."]
    ],
    "no-return-await": false,
    "class-name": false,
    "curly": false,
    "interface-name": [false, "never-prefix"],
    "jsdoc-format": false,
    "forin": false,
    "label-position": false,
    "max-line-length": {
      "options": {"limit": 80, "ignore-pattern": "^import |^export |https?://"}
    },
    "new-parens": false,
    "no-angle-bracket-type-assertion": false,
    "no-any": false,
    "no-construct": false,
    "no-const-assign": "warn",
    "no-this-before-super": "warn",
    "no-consecutive-blank-lines": false,
    "no-debugger": false,
    "no-default-export": false,
    "no-imports-from-dist": false,
    "no-inferrable-types": false,
    "no-namespace": [false, "allow-declarations"],
    "no-reference": false,
    "no-require-imports": false,
    "no-string-throw": false,
    "no-unused-expression": false,
    "no-var-keyword": false,
    "no-unnecessary-type-assertion": false,
    "object-literal-shorthand": false,
    "only-arrow-functions": [false, "allow-declarations", "allow-named-functions"],
    "prefer-const": false,
    "quotemark": [false, "single"],
    "radix": false,
    "restrict-plus-operands": false,
    "semicolon": [false, "always", "ignore-bound-class-methods"],
    "switch-default": false,
    "triple-equals": [false, "allow-null-check"],
    "use-isnan": false,
    "use-default-type-parameter": false,
    "variable-name": [
      false,
      "check-format",
      "ban-keywords",
      "allow-leading-underscore",
      "allow-trailing-underscore"
    ]
  }
}
